{
  "name": "portafolio",
  "version": "1.0.0",
  "description": "Proyecto de portafolio Jefferson Andres Moreno Pedraza",
  "main": "tailwind.config.js",
  "scripts": {
    "build": "node ./node_modules/.bin/tailwindcss -i ./input.css -o ./assets/output.css --minify",
    "dev": "node ./node_modules/.bin/tailwindcss -i ./input.css -o ./assets/output.css --watch",
    "postinstall": "chmod +x ./node_modules/.bin/tailwindcss"
  },
  "author": "Jefferson Andres Moreno Pedraza",
  "license": "ISC",
  "dependencies": {
    "@tailwindcss/cli": "^4.1.16",
    "tailwindcss": "^4.1.16"
  }
}
